{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/rest/(.*)", "dest": "/api/rest/$1" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ],
  "functions": {
    "api/rest/no.js": {
      "maxDuration": 10
    }
  }
}
